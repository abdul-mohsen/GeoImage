<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/imageList">

    <fragment
        android:id="@+id/imageList"
        android:name="com.bignerdranch.android.geoimage.control.ImageList"
        android:label="ImageList"
        tools:layout="@layout/fragment_image_list">
        <action
            android:id="@+id/action_imageList_to_imagePreview"
            app:destination="@id/imagePreview" />
    </fragment>
    <fragment
        android:id="@+id/imagePreview"
        android:name="com.bignerdranch.android.geoimage.control.ImagePreview"
        android:label="ImagePreview"
        tools:layout="@layout/fragment_preview">
        <argument
            android:name="url"
            app:argType="string" />
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
</navigation>